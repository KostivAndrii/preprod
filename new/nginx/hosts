# Application servers
[centos]
centos ansible_host=192.168.60.160
[ubuntu]
ubuntu ansible_host=192.168.60.150
# Variables that will be applied to all servers
[all:vars]
ansible_ssh_user=vagrant
ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key
